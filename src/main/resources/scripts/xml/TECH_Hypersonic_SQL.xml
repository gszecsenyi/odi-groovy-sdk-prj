	<Field name="EncKey" type="java.lang.String">null</Field>
	<Field name="FullTxt" type="java.lang.String"><![CDATA[select   index_name,
      column_name
 from INFORMATION_SCHEMA.SYSTEM_INDEXINFO idx
where not exists (
select 1 from INFORMATION_SCHEMA.table_constraints cons
where
idx.table_name=cons.table_name
and index_name=cons.constraint_name or
RIGHT(index_name,length(index_name)-8) like concat(cons.constraint_name,'%'))
and table_name=:TABLE ]]></Field>
