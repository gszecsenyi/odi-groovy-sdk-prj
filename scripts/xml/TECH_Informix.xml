	<Field name="FullTxt" type="java.lang.String"><![CDATA[
select	
	pktab.tabname PK_TABLE_NAME,
	trim(cons.constrname)	FK_NAME,
	trim(col.colname)	FK_COLUMN_NAME,
	trim(pkcol.colname) PK_COLUMN_NAME
from 	informix.sysindexes idx,
	informix.systables tab,
	informix.syscolumns col,
	informix.sysconstraints cons,
	informix.sysreferences ref,
	informix.systables pktab,
	informix.sysconstraints pkcons,
	informix.sysindexes pkidx,
	informix.syscolumns pkcol
where	idx.tabid	=tab.tabid
and	((col.colno=idx.part1 and col.tabid=idx.tabid    and pkcol.colno=pkidx.part1 and pkcol.tabid=pkidx.tabid )
	or (col.colno=idx.part2 and col.tabid=idx.tabid and pkcol.colno=pkidx.part2 and pkcol.tabid=pkidx.tabid )
	or (col.colno=idx.part3 and col.tabid=idx.tabid and pkcol.colno=pkidx.part3 and pkcol.tabid=pkidx.tabid )
	or (col.colno=idx.part4 and col.tabid=idx.tabid and pkcol.colno=pkidx.part4 and pkcol.tabid=pkidx.tabid )
	or (col.colno=idx.part5 and col.tabid=idx.tabid and pkcol.colno=pkidx.part5 and pkcol.tabid=pkidx.tabid )
	or (col.colno=idx.part6 and col.tabid=idx.tabid and pkcol.colno=pkidx.part6 and pkcol.tabid=pkidx.tabid )
	or (col.colno=idx.part7 and col.tabid=idx.tabid and pkcol.colno=pkidx.part7 and pkcol.tabid=pkidx.tabid )
	or (col.colno=idx.part8 and col.tabid=idx.tabid and pkcol.colno=pkidx.part8 and pkcol.tabid=pkidx.tabid )
	)
and cons.idxname = idx.idxname
and cons.constrtype = 'R'
and ref.constrid = cons.constrid
and ref.ptabid = pktab.tabid
and ref.primary = pkcons.constrid
and pkcons.idxname = pkidx.idxname
and	tab.tabname = :TABLE
and TAB.OWNER = :SCHEMA
]]></Field>
