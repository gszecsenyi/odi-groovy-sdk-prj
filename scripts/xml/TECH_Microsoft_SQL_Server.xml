<Object class="com.sunopsis.dwg.dbobj.SnpMtxt">
		<Field name="Enc" type="java.lang.String">null</Field>
	<Field name="EncKey" type="java.lang.String">null</Field>
	<Field name="FullTxt" type="java.lang.String"><![CDATA[select I.name index_name, index_col(:CATALOG + '.' + :SCHEMA + '.' + :TABLE, K.indid, K.keyno) column_name
from sysindexes I , sysindexkeys K
where I.id = object_id (:CATALOG + '.' + :SCHEMA + '.' + :TABLE)
and I.indid > 0
and I.indid < 255
and (I.status & 64)=0
and I.id=K.id
and I.indid=K.indid
order by I.indid]]></Field>
	<Field name="ITxt" type="com.sunopsis.sql.DbInt"><![CDATA[8999]]></Field>
